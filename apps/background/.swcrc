{
	"minify": true,
	"jsc": {
		"minify": {
			"compress": {
				"drop_console": true
			}
		},
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true,
			"dynamicImport": true
		},
		"target": "es2020",
		"transform": {
			"react": {
				"pragma": "React.createElement",
				"pragmaFrag": "React.Fragment",
				"throwIfNamespace": true,
				"development": false,
				"useBuiltins": false
			}
		}
	},
	"sourceMaps": false
}